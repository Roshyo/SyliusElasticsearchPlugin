{
    "name": "setono/sylius-elasticsearch-plugin",
    "type": "sylius-plugin",
    "description": "Elasticsearch plugin for Sylius",
    "keywords": ["sylius", "elasticsearch", "search"],
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "sylius/sylius": "^1.3.0",
        "friendsofsymfony/elastica-bundle": "dev-master"
    },
    "require-dev": {
        "sylius-labs/coding-standard": "^2.0"
    },
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Setono\\SyliusElasticsearchPlugin\\": "src/"
        }
    },
    "scripts": {
        "check-style": "./vendor/bin/ecs check src/",
        "fix-style": "./vendor/bin/ecs check --fix src/"
    }
}
